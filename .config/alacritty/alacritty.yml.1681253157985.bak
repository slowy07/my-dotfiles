import:
  - ./colors-material.yml

window:
  padding:
    x: 10
    y: 10

  decorations: none

  opacity: 1.0
  dynamic_title: true

  history: 10000
  multiplier: 3

font:
  normal:
    family: Agave Nerd Font
    #style: Regular

  bold:
    family: Agave Nerd Font
    #style: Bold

  italic:
    family: Agave Nerd Font
    #style: Italic

  bold_italic:
    family: Agave Nerd Font
    #style: Bold Italic

  size: 9
  offset:
    x: -2
    y: -2

# Colors (Decay, decayce variant) z0mbi3 Rice
selection:
  save_to_clipboard: false

cursor:
  style:
    shape: Block
    blinking: On

  unfocused_hollow: false
  blink_interval: 550

live_config_reload: true
colors:
  name: Atelierheath (dark)
  author: Chris Kempson
  primary:
    background: "#1b181b"
    foreground: "#ab9bab"
  cursor:
    text: "#1b181b"
    cursor: "#ab9bab"
  normal:
    black: "#1b181b"
    red: "#ca402b"
    green: "#379a37"
    yellow: "#bb8a35"
    blue: "#516aec"
    magenta: "#7b59c0"
    cyan: "#159393"
    white: "#ab9bab"
  bright:
    black: "#776977"
    red: "#ca402b"
    green: "#379a37"
    yellow: "#bb8a35"
    blue: "#516aec"
    magenta: "#7b59c0"
    cyan: "#159393"
    white: "#f7f3f7"
theme: Atelierheath.dark
