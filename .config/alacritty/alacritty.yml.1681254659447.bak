import:
  - ./colors-material.yml

window:
  padding:
    x: 10
    y: 10

  decorations: none

  opacity: 1.0
  dynamic_title: true

  history: 10000
  multiplier: 3

font:
  normal:
    family: Agave Nerd Font
    #style: Regular

  bold:
    family: Agave Nerd Font
    #style: Bold

  italic:
    family: Agave Nerd Font
    #style: Italic

  bold_italic:
    family: Agave Nerd Font
    #style: Bold Italic

  size: 9
  offset:
    x: -2
    y: -2

# Colors (Decay, decayce variant) z0mbi3 Rice
selection:
  save_to_clipboard: false

cursor:
  style:
    shape: Block
    blinking: On

  unfocused_hollow: false
  blink_interval: 550

live_config_reload: true
colors:
  name: Atelierlakeside (light)
  author: Chris Kempson
  primary:
    background: "#ebf8ff"
    foreground: "#516d7b"
  cursor:
    text: "#ebf8ff"
    cursor: "#516d7b"
  normal:
    black: "#161b1d"
    red: "#d22d72"
    green: "#568c3b"
    yellow: "#8a8a0f"
    blue: "#257fad"
    magenta: "#5d5db1"
    cyan: "#2d8f6f"
    white: "#7ea2b4"
  bright:
    black: "#5a7b8c"
    red: "#d22d72"
    green: "#568c3b"
    yellow: "#8a8a0f"
    blue: "#257fad"
    magenta: "#5d5db1"
    cyan: "#2d8f6f"
    white: "#ebf8ff"
theme: Atelierlakeside.light
